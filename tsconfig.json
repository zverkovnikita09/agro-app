{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@images/*": ["assets/images/*"],
      "@fonts/*": ["assets/fonts/*"],
      "@features/*": ["src/features/*"],
      "@pages/*": ["src/pages/*"],
      "@shared/*": ["src/shared/*"],
      "@entities/*": ["src/entities/*"],
      "@widgets/*": ["src/widgets/*"],
      "@app/*": ["src/core/*"],
    }
  }
}
